# example
# radakan_path=c:/games/radakan
# ogre_media_path=c:/ogre3d/media
[directories]
radakan_path=C:/games/radakan
ogre_media_path=C:/OgreSDK/media

[keyboard]
forward=w
back=s
left=a
right=d

[gameplay]
# these affect both left and right turns, no negative numbers as the sign is automatically applied.
turn_speed = 1

# no negative numbers as the negative sign is automatically applied
foreward_movement_speed = 1.0
backward_movement_speed = 0.7


