# example
# radakan_path=c:/games/radakan
# ogre_media_path=c:/ogre3d/media
[directories]
radakan_path=C:/games/radakan
ogre_media_path=C:/OgreSDK/media

[key bindings]
go forward=w
go backward=s
turn left=a
turn right=d
select target=t
hit=h
give=g
jump=space
reset=r
menu=escape

[gameplay]
# These affect all characters.
movement_reaction = 500
maximal_movement_speed = 4
turn_reaction = 50
maximal_turn_speed = 4
